extends ../includes/layout.pug

append resources
  title Users
  
append body
  +header('Users')
  +navvy(1)
  +row(8, 2)
    ul.list-group.list-group-flush
      each user, index in users
        li.list-group-item
          a.form-control(href='/user?id=' + user.id  class='btn btn-link')= user.name
